{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./",
    "paths": {
      "@reaper/*":["./node_modules/reaperjs/*"],
      "@reaper":["./node_modules/reaperjs/client.ts","./node_modules/reaperjs/index.ts"],
      "@models/*": ["db/models/*"],
      "@global/*": ["global/*"],
      "@hooks/*": ["hooks/*"],
      "@styles/*": ["views/styles/*"],
      "@components/*": ["views/components/*"],
      "@ts/*": ["types/*"]
    }
  }
}
