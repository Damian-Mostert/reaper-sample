{
  "version": 2,
  "builds": [
    {
      "src": "vercel/api.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["node_modules/ejs/**/*"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/vercel/api.js"
    }
  ],
  "buildCommand": "npm i && npm run vercel"
}

